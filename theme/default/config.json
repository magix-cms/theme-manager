{
  "name": "default",
  "description": "Default theme for Magix CMS",
  "version": "1.0.0",
  "keywords": [
    "css",
    "less",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web",
    "Magix CMS"
  ],
  "authors": [
    "Salvatore Di Salvo"
  ],
  "license": "MIT",
  "style": "css/style.min.css",
  "engines": {
    "Magix CMS": ">=<%= mc_version%>"
  },
  "cssProcessor": "less",
  "paths": {
    "bowerDir": "./bower_components",
    "skins": "../skin",
    "themePath": {
      "css": "/css",
      "cssSrc": "/css/src",
      "less": "/css/src/less",
      "scss": "/css/src/scss",
      "fonts": "/css/fonts",
      "vendorsSrc": "/js/vendor/src",
      "vendors": "/js/vendor",
      "jsSrc": "/js/src",
      "js": "/js"
    }
  },
  "plugins": {},
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "fontawesome#^4.7.0",
    "jquery": "^3.1.1",
    "fancybox": "^2.1.6"
  }
}